package noinheri;

public class Rabbit {
	// 토끼 몬스터 hp, atk, def
	private int hp;
	private int atk;
	private int def;

	// 생성자에서 void 파라미터로 각각 3, 0, 0으로 초기화
	public Rabbit() {
		this.hp = 3;
		this.atk = 0;
		this.def = 0;
	}
	
	// setter / getter 자동생성
	// alt + shift + s 후 Generate getters and setters 선택
	
}
