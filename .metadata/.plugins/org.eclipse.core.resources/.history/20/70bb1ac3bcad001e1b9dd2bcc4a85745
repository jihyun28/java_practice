package this_;

public class Human {
	public String name;
	public int age;
	
	// 생성자 선언 
	// this 키워드 -> 전달받은 값을 멤버변수에 대입
	public Human(String name, int age) {
		this.name = name;
		this.age = age;
	}
	
	// 메서드 오버로딩으로 이름만 입력받는 생성자 정의
	public Human(String name) {
		this(name, 0); // 아래 두 줄과 같은 의미
		//this.name = name;
		//this.age = 0; 
	}
}
